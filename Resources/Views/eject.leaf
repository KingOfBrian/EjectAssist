#extend("base")

#export("head") {}

#export("body") {
  <div class="message">
    <h1>Eject from Interface Builder</h1>

    Eject will generate swift code to replace your .xib file.

    <form action="eject" method="POST" enctype="multipart/form-data">
      <input type="file" name="xib">
      <input type="checkbox" name="saveXML" value="YES" checked="checked">Make the XML and code public to help improve the product<br>
      <input type="submit" value="Eject">
    </form>
  </div>
}
